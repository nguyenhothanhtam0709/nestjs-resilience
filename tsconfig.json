{
	"compilerOptions": {
		"target": "ES2023",
		"module": "commonjs",
		"outDir": "./dist",
		"baseUrl": "./",
		"declaration": true,
		"noImplicitAny": false,
		"noUnusedLocals": false,
		"removeComments": false,
		"noLib": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"sourceMap": false,
		"skipLibCheck": true
	},
}
